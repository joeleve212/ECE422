	.cdecls C, list, "msp430.h"	; this allows us to use C headers
	
	.text						; locates code in 'text' section
	
	.global multiply				; 'comp' is now global
	
multiply:
	;TODO: use MPY32 to multiply r12 & r13
	mov.w r12, &MPY
	mov.w r13, &OP2
	;
	;TODO: other?
	;mov.w r12,r13 
	reta ;return that value
	
	
	
	.end
	